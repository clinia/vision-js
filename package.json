{
  "name": "@clinia/vision",
  "version": "1.0.0-beta.0",
  "description": "VisionJS is a JavaScript library for building performant and healthcare search experiences with Clinia.",
  "keywords": [
    "clinia",
    "vision",
    "search",
    "widget",
    "vanilla"
  ],
  "author": "Clinia <support@clinia.com>",
  "license": "MIT",
  "repository": "clinia/vision-js",
  "main": "cjs/index.js",
  "module": "es/index.js",
  "jsdelivr": "dist/vision.production.min.js",
  "unpkg": "dist/vision.production.min.js",
  "sideEffects": false,
  "scripts": {
    "start": "yarn run dev",
    "dev": "yarn run storybook",
    "build": "yarn run build:cjs && yarn run build:es && yarn run build:umd",
    "build:umd": "BABEL_ENV=umd rollup --config scripts/rollup/config.js",
    "build:cjs": "BABEL_ENV=cjs babel src --extensions '.js,.ts,.tsx' --out-dir cjs/ --ignore 'src/index.es.ts','**/__tests__','**/__mocks__' --quiet",
    "build:es": "BABEL_ENV=es babel src --extensions '.js,.ts,.tsx' --out-dir es/ --ignore 'src/index.es.ts','**/__tests__','**/__mocks__' --quiet && BABEL_ENV=es babel src/index.es.ts --out-file es/index.js --quiet",
    "build:types": "tsc -p tsconfig.declaration.json --outDir es/; mv es/index.es.d.ts es/index.d.ts",
    "doctoc": "doctoc --no-title --maxlevel 3 README.md CONTRIBUTING.md",
    "storybook": "start-storybook --quiet --port 6006 --ci --static-dir .storybook/static",
    "storybook:build": "build-storybook --quiet --output-dir website/stories --static-dir .storybook/static",
    "website:build": "yarn build && ./scripts/website/build-examples.sh",
    "lint": "eslint --ext .js,.ts,.tsx .",
    "lint:fix": "eslint --ext .js,.ts,.tsx --fix .",
    "type-check": "tsc",
    "type-check:js": "tsc --project tsconfig.checkjs.json",
    "type-check:watch": "yarn type-check --watch",
    "test": "jest",
    "test:watch": "jest --watch --bail",
    "test:e2e": "yarn test:e2e:local",
    "test:e2e:local": "wdio wdio.local.conf.js",
    "test:e2e:saucelabs": "wdio wdio.saucelabs.conf.js",
    "test:size": "bundlesize",
    "test:argos": "argos upload functional-tests/screenshots --token $ARGOS_TOKEN || true",
    "release": "./scripts/release/publish.js",
    "release:maintenance": "./scripts/release/publish-maintenance.sh",
    "release:view-changelog": "conventional-changelog -p angular",
    "release:prepare": "shipjs prepare",
    "release:trigger": "shipjs trigger"
  },
  "files": [
    "dist",
    "cjs",
    "es",
    "scripts/transforms"
  ],
  "dependencies": {
    "@clinia/search-helper": "2.0.0-beta.0",
    "classnames": "2.2.6",
    "events": "3.1.0",
    "hogan.js": "3.0.2",
    "preact": "10.3.1",
    "prop-types": "15.7.2",
    "qs": "6.9.1"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.8.4",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-transform-react-constant-elements": "7.8.3",
    "@babel/preset-env": "7.8.4",
    "@babel/preset-react": "7.8.3",
    "@babel/preset-typescript": "7.8.3",
    "@clinia/client-search": "2.0.0-beta.1",
    "@storybook/addon-actions": "5.3.13",
    "@storybook/html": "5.3.13",
    "@storybook/theming": "5.3.13",
    "@testing-library/jest-dom": "5.1.1",
    "@testing-library/preact": "1.0.2",
    "@types/classnames": "2.2.9",
    "@types/enzyme": "3.10.5",
    "@types/jest": "25.1.2",
    "@types/jest-diff": "24.3.0",
    "@types/qs": "6.9.1",
    "@types/storybook__addon-actions": "5.2.1",
    "@typescript-eslint/eslint-plugin": "2.19.2",
    "@typescript-eslint/parser": "2.19.2",
    "@wdio/cli": "5.18.6",
    "@wdio/jasmine-framework": "5.18.6",
    "@wdio/local-runner": "5.18.6",
    "@wdio/sauce-service": "5.16.10",
    "@wdio/selenium-standalone-service": "5.16.10",
    "@wdio/spec-reporter": "5.18.6",
    "@wdio/static-server-service": "5.16.10",
    "babel-eslint": "10.0.3",
    "babel-jest": "25.1.0",
    "babel-loader": "8.0.6",
    "babel-plugin-inline-replace-variables": "1.3.1",
    "babel-plugin-transform-react-pure-class-to-function": "1.0.1",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "bundlesize": "0.18.0",
    "clinia": "2.0.0-beta.1",
    "colors": "1.4.0",
    "conventional-changelog-cli": "2.0.31",
    "doctoc": "1.4.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-preact-pure": "2.2.0",
    "enzyme-to-json": "3.4.4",
    "eslint": "6.8.0",
    "eslint-config-clinia": "2.0.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-import-resolver-webpack": "0.12.1",
    "eslint-plugin-eslint-comments": "3.1.2",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jasmine": "4.1.0",
    "eslint-plugin-jest": "23.7.0",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.18.3",
    "eslint-plugin-react-hooks": "2.3.0",
    "inquirer": "7.0.4",
    "jest": "25.1.0",
    "jest-diff": "25.1.0",
    "jest-environment-jsdom": "25.1.0",
    "jest-environment-jsdom-global": "1.2.1",
    "jest-watch-typeahead": "0.4.2",
    "jscodeshift": "0.7.0",
    "jsdom-global": "3.0.2",
    "mversion": "1.13.0",
    "prettier": "1.19.1",
    "rollup": "1.31.0",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-filesize": "6.2.1",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-replace": "2.2.0",
    "rollup-plugin-uglify": "6.0.4",
    "scriptjs": "2.5.9",
    "semver": "7.1.3",
    "shelljs": "0.8.3",
    "shipjs": "0.15.0",
    "ts-essentials": "6.0.1",
    "typescript": "3.7.5",
    "webpack": "4.41.6"
  },
  "bundlesize": [
    {
      "path": "./dist/vision.production.min.js",
      "maxSize": "63.25 kB"
    },
    {
      "path": "./dist/vision.development.js",
      "maxSize": "160 kB"
    }
  ]
}
